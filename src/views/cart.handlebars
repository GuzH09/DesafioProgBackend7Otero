<h1>ID Carrito: {{id}}</h1>
<div>
    {{#each products}}
        <article>
            <h1>{{this.product.title}}</h1>
            <p>Cantidad: {{this.quantity}}</p>
            <p>Precio: {{this.product.price}}</p>
            <p>Stock: {{this.product.stock}}</p>
            <p>Categoria: {{this.product.category}}</p>
        </article>
    {{/each}}
</div>